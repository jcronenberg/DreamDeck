; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "DreamDeckConstants",
"language": "GDScript",
"path": "res://scripts/global/Constants.gd"
}, {
"base": "Reference",
"class": "HttpRequest",
"language": "GDScript",
"path": "res://plugins/SpotifyPanel/scripts/godottpd/http_request.gd"
}, {
"base": "Reference",
"class": "HttpResponse",
"language": "GDScript",
"path": "res://plugins/SpotifyPanel/scripts/godottpd/http_response.gd"
}, {
"base": "Node",
"class": "HttpServer",
"language": "GDScript",
"path": "res://plugins/SpotifyPanel/scripts/godottpd/http_server.gd"
} ]
_global_script_class_icons={
"DreamDeckConstants": "",
"HttpRequest": "",
"HttpResponse": "",
"HttpServer": ""
}

[application]

config/name="DreamDeck"
run/main_scene="res://scenes/Main.tscn"
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=20700
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://resources/general/dreamdeck.png"
config/windows_native_icon="res://resources/general/dreamdeck.ico"

[autoload]

ArgumentParser="*res://scripts/AutoLoad/ArgumentParser.gd"
ConfigLoader="*res://scripts/AutoLoad/ConfigLoader.gd"
GlobalSignals="*res://scripts/AutoLoad/GlobalSignals.gd"
HandleConfig="*res://scripts/AutoLoad/HandleConfig.gd"
PluginLoader="*res://scripts/AutoLoad/PluginLoader.gd"

[debug]

settings/fps/force_fps=30
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/vsync/use_vsync=false
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/energy_saving/keep_screen_on=false

[editor]

main_run_args="--confdir=local_config"

